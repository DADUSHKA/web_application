<div id="container">
  <div id="header">TestGuru</div>
  <div id="sidebar">
    <%= link_to t('.new_category'), new_admin_category_path %>
    <p></p>
      <%= link_to t('.new_test'), new_admin_test_path %>
  </div>
  <div id="content">
    <h2><%= t('.header') %></h2>
    <table>
    <tr>
      <th><%= t('.title') %></th>
      <th><%= t('.level') %></th>
      <th><%= t('.author') %></th>
      <th><%= t('.category') %></th>
      <th><%= t('.total_question') %></th>
    <th colspan="3"><%= t('.actions') %></th>
    </tr>
      <%= render @tests %>
    </table>
  </div>
</div>
